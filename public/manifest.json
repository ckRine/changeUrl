{
  "manifest_version": 3,
  "name": "Change URL Extension",
  "version": "1.0",
  "description": "Extension pour modifier les URL",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"],
      "css": ["index.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.html", "index.js", "index.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
"extension_pages": "script-src 'self'; object-src 'self'"
}

}