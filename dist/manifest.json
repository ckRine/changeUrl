{
       "manifest_version": 3,
       "name": "ChangeUrl",
       "version": "5.0.0",
       "description": "A browser extension to modify and redirect URLs with custom buttons.",
       "permissions": [
         "tabs",
         "activeTab",
         "storage"
       ],
       "action": {
         "default_popup": "index.html"
       },
       "icons": {
         "48": "icon48.png",
         "128": "icon128.png"
       }
     }